<%- include("includes/head.ejs")%>

    <body>
        <%- include("includes/navigation.ejs")%>

            <main>
                <h1>Welcome!</h1>

                <% if (users.length>0) { %>
                    <h3>Available Users</h3>

                    <% for (let user of users){ %>
                        <ul>
                            <li>
                                <%= user.userName %>
                            </li>
                        </ul>
                        <% } %>

                            <% } else { %>
                                <p>No Users found!</p>
                                <% } %>
            </main>
    </body>

    </html>